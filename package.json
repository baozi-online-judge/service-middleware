    "jsonwebtoken": "^8.5.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^4.43.0",
    "sequelize-typescript": "^0.6.7",
    "sqlite3": "^4.0.6",
    "vm2": "^3.7.0"
